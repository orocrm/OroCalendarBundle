{
    "name": "oro/calendar-bundle",
    "type": "symfony-bundle",
    "description": "Calendar bundle for OroPlatform-based applications.\nSee https://oroinc.com/orocrm/doc/current/user-guide/activities/calendars-overview for more information.",
    "homepage": "https://github.com/oroinc/OroCalendarBundle.git",
    "keywords": ["Oro","OroPlatform","OroCRM","Calendar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "require": {
        "oro/platform": "~3.1.1",
        "bower-asset/fullcalendar": "3.4.0"
    },
    "autoload": {
        "psr-4": { "Oro\\Bundle\\CalendarBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.oroinc.com"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.oroinc.com"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    }
}
